{
  "mcpServers": {
    "nuxt": {
      "type": "http",
      "url": "https://nuxt.com/mcp"
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "studio Docs": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://gitmcp.io/nuxt-content/studio"
      ]
    }
  }
}